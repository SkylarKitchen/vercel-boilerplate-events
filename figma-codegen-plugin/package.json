{
  "name": "figma-codegen-plugin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite build --watch --config vite.config.plugin.ts\" \"vite build --watch --config vite.config.ui.ts\"",
